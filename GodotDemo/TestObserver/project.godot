;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="TestObserver"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GlobalNode="*res://Scenes/Nodes/GlobalNode.cs"
GlobalNodeGds="*res://Scenes/Nodes/GDScripts/global_node_gds.gd"

[dotnet]

project/assembly_name="TestObserver"

[file_customization]

folder_colors={
"res://Assets/": "yellow",
"res://Scenes/": "green",
"res://Shaders/": "purple"
}
